#include <iostream>
using namespace std;
int main() {
    int x,y,z,w,p,i;
scanf("%x",&x); // 1 задание

printf("%o%c", x,'\n'); // 2 задание
scanf("%o",&y);

w = y << 1;                   // 3 задание
printf("%o%c",y,' ');
printf("%o%c%c",w,' ','\n'); //
z = ~x;                               // 4 задание
printf("%x%c",x,' ');
printf("%x%c%c",z,' ','\n');

scanf("%o",&p); // 5 задание
i = p & x;
printf("%o",i);
    return 0;
}
